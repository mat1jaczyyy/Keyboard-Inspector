<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net9.0-windows</TargetFramework>
    <OutputType>WinExe</OutputType>
    <RootNamespace>Keyboard_Inspector</RootNamespace>
    <StartupObject>Keyboard_Inspector.Program</StartupObject>
    <UseWindowsForms>true</UseWindowsForms>
    <ImportWindowsDesktopTargets>true</ImportWindowsDesktopTargets>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <Platforms>x64</Platforms>
    <PlatformTarget>x64</PlatformTarget>
    <ApplicationIcon>keyboard.ico</ApplicationIcon>
    <RuntimeIdentifier>win-x64</RuntimeIdentifier>
    <PublishSingleFile>true</PublishSingleFile>
  </PropertyGroup>
  <PropertyGroup>
    <GenerateAssemblyInfo>true</GenerateAssemblyInfo>
    <Title>Keyboard Inspector</Title>
    <Authors>mat1jaczyyy</Authors>
    <Company>$(Authors)</Company>
    <Copyright>Copyright © 2021 - 2024</Copyright>
    <PackageProjectUrl>https://github.com/mat1jaczyyy/Keyboard-Inspector</PackageProjectUrl>
    <RepositoryUrl>https://github.com/mat1jaczyyy/Keyboard-Inspector</RepositoryUrl>
    <AssemblyVersion>1.0.2.0</AssemblyVersion>
    <FileVersion>1.0.2.0</FileVersion>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="DynaJson" Version="2.2.0" />
    <PackageReference Include="FFTW.NET" Version="1.0.1" />
    <PackageReference Include="Grille-DarkUI" Version="3.0.2" />
    <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
  </ItemGroup>
  <ItemGroup>
    <EditorConfigFiles Remove=".editorconfig" />
  </ItemGroup>
  <ItemGroup>
    <None Include=".editorconfig" />
  </ItemGroup>
</Project>